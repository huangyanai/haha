package cn.spark.study.core

import org.apache.spark.SparkContext
import org.apache.spark.SparkConf

object Text2 {
   def main(args:Array[String]){
    val conf=new SparkConf().setAppName("Text").setMaster("local")
    val sc=new SparkContext(conf)
   def groupByKeyTest(){
      //tuple(元组)可以是不同的数据类型
      val scoreList=Array(Tuple2("class1",90),Tuple2("class2",80),Tuple2("class1",75),Tuple2("class2",90))
      val arrRDD=sc.parallelize(score,2)
      val result=arrRDD.map(x=>x*4)//map为转换操作，还需要一个行动操作来启动
      result.foreach(x=>print(x+","))
    }
   }
}